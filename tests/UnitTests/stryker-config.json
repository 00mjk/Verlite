{
	"stryker-config": {
		"reporters": [
			"progress",
			"html"
		],
		"mutate": [
			"!*Exception.cs"
		],
		"ignore-methods": [
			"ILogger.*",
			"Normal",
			"Verbose",
			"Verbatim",
			"*.Assert",
			"*HashCode",
			"*Exception.ctor",
			"First",
			"System.Text.Regex.RegexOptions.op_BitwiseOr"
		],
		"threshold-break": 0
	}
}
